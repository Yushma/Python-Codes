   "source": [
    "def factorial_num(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial_num(n-1)\n",
    "    \n",
    "    \n",
    "n=5\n",
    "factorial_num(5)\n",
