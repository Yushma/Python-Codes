   "source": [
    "# Write a Python program to find the sum of the first n natural numbers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "94969ccc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any number: 5\n",
      "Sum of given 5 numbers is : 15\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter any number: \"))\n",
    "sum = n*(n+1)//2\n",
    "\n",
    "print(f\"Sum of given {n} numbers is : {sum}\")"
   ]
