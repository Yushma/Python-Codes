   "source": [
    "num = int(input())\n",
    "def factorial(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "    \n",
    "print(f\"Factorial of a given {num} = {factorial(num)}\")"
   ]
