   "source": [
    "def is_palindrome(s):\n",
    "    if s==s[::-1]:\n",
    "        return \"Given string is a palindrome\"\n",
    "    else:\n",
    "        return \"Given string is not a Palindrome\"\n",
    "\n",
    "s= \"madam\"\n",
    "is_palindrome(s)"
