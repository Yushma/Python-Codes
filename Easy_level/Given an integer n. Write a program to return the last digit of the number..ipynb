   ],
   "source": [
    "class Solution:\n",
    "    def lastDigit(self, n: int) -> int:\n",
    "        #Code here\n",
    "        last_dig = abs(n)%10\n",
    "        return last_dig;\n",
    "        \n",
    "Solution1 = Solution()\n",
    "Solution1.lastDigit(9768)"
