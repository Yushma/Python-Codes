   "source": [
    "###Write a program that calculates the factorial of a number input by the user using a while loop\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "c5d5a0f1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any number: 5\n",
      "Factorial of a given number : 120\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter any number: \"))\n",
    "fact =1\n",
    "i =1\n",
    "while(i<=n):\n",
    "    fact *= i\n",
    "    i +=1\n",
    "print(f\"Factorial of a given number : {fact}\")"
   ]
  },
