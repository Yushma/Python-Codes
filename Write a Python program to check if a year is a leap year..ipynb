   "source": [
    "# Write a Python program to check if a year is a leap year."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "9032cc41",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter any year : 2000\n",
      "Leap year\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter any year : \"))\n",
    "if (n%4 == 0 and n%100 != 0) or (n%400 == 0):\n",
    "    print(\"Leap year\")\n",
    "else:\n",
    "    print(\"Not a Leap year\")"
   ]
